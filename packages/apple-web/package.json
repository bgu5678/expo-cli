{
  "name": "setup-safari",
  "version": "0.0.0",
  "main": "build",
  "keywords": [
    "expo",
    "web"
  ],
  "description": "Setup Safari credentials for Expo projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/expo/expo-cli.git"
  },
  "homepage": "https://github.com/expo/expo-cli/tree/main/packages/apple-web",
  "author": "Expo <support@expo.dev>",
  "license": "MIT",
  "bin": {
    "setup-safari": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "prepare": "yarn run clean && yarn run build:prod",
    "lint": "eslint .",
    "watch": "tsc --watch --preserveWatchOutput",
    "watch:ncc": "yarn run build -w",
    "build": "ncc build ./src/index.ts -o build/",
    "build:prod": "ncc build ./src/index.ts -o build/ --minify --no-cache --no-source-map-register",
    "clean": "rimraf ./build/"
  },
  "devDependencies": {
    "@expo/apple-utils": "1.2.0",
    "@expo/config": "^8.0.2",
    "chalk": "^4.0.0",
    "commander": "2.20.0"
  }
}
